    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="i" type="Integer" array="False" size=""/>
            <declare name="valor" type="Integer" array="False" size=""/>
            <declare name="pares" type="Integer" array="False" size=""/>
            <assign variable="pares" expression="0"/>
            <for variable="i" start="1" end="100" direction="inc" step="1">
                <output expression="&quot;Ingresa un valor entero &quot; &amp; i &amp; &quot;/100&quot;" newline="True"/>
                <input variable="valor"/>
                <if expression="valor % 2 == 0">
                    <then>
                        <assign variable="pares" expression="pares + 1"/>
                    </then>
                    <else/>
                </if>
            </for>
            <output expression="&quot;En total hay &quot; &amp; pares &amp; &quot; numeros pares ingresados&quot;" newline="True"/>
        </body>
    </function>
